{
    "name": "backbrain/php-automapper",
    "description": "PHP port of the popular automapper.org library",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Backbrain\\Automapper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Backbrain\\Automapper\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Maximilian Reichel",
            "email": "hello@backbrain.io"
        }
    ],
    "require": {
        "php": ">=8.2",
        "psr/log": "^3.0",
        "psr/cache": "^3.0",
        "symfony/property-access": "7.*",
        "symfony/property-info": "7.*",
        "phpstan/phpdoc-parser": "^1.26",
        "phpdocumentor/type-resolver": "^1.8",
        "phpdocumentor/reflection-docblock": "^5.3",
        "jawira/case-converter": "^3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "phpstan/phpstan": "^1.10",
        "friendsofphp/php-cs-fixer": "^3.51",
        "symfony/var-dumper": "*",
        "doctrine/collections": "^2.2"
    }
}
